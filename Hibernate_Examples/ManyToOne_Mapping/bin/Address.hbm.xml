<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

  <class name="com.training.Address" table="myaddress">
  <id name="address_id" column="Address_id" type="int">
  <generator class="assigned"></generator>
  </id>
  <property name="street" column="Street" type="string"></property>
  <property name="nagar" column="Nagar" type="string"> </property>
  <property name="city" column="City" type="string"></property>
  <property name="pincode" column="Pincode" type="string"></property>
  
  
  </class>
</hibernate-mapping>